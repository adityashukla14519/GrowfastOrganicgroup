<script>
    function downloadinvoice(invoiceId, horderid, horderid2, rowid, mobile) {
        const element = document.getElementById('invoice1');
        element.style.padding = '20px';

        html2pdf().from(element).toPdf().get('pdf').then(function (pdf) {
            // Convert the PDF blob to a Blob URL
            var blobUrl = URL.createObjectURL(pdf.output('blob'));

            // Send the PDF data to the server
            var xhr = new XMLHttpRequest();
            xhr.open('POST', '/SavePdfHandler.ashx?invoiceId=' + encodeURIComponent(invoiceId) + '&horderid=' + encodeURIComponent(horderid) + '&horderid2=' + encodeURIComponent(horderid2) + '&rowid=' + encodeURIComponent(rowid) + '&mobile=' + encodeURIComponent(mobile), true);
            xhr.setRequestHeader('Content-type', 'application/pdf');
            xhr.responseType = 'blob';

            xhr.onload = function () {
                if (xhr.status === 200) {
                    // Create a temporary link element
                    var a = document.createElement('a');
                    var blob = new Blob([xhr.response], { type: 'application/pdf' });
                    var url = window.URL.createObjectURL(blob);

                    a.href = url;
                    a.download = 'invoice.pdf'; // Set the file name

                    // Prompt the user to choose the location and save the file
                    document.body.appendChild(a);
                    a.click();

                    // Clean up
                    document.body.removeChild(a);
                    window.URL.revokeObjectURL(url);
                }
            };

            xhr.send(pdf.output('blob'));
        });

        element.style.padding = '0';
    }
</script>


